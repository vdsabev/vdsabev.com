  "name": "vdsabev.com",
  "scripts": {
    "start": "cmless start",
    "build": "cmless build",
    "e2e": "cypress run",
    "e2e.watch": "cypress open"
  },
  "dependencies": {
    "@hyperapp/router": "0.4.1",
    "classcat": "3.0.2",
    "overstate": "1.2.2",
    "picodom": "2.0.0"
